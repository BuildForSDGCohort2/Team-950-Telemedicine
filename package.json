{
  "name": "telemedicine",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "eslint": "eslint \"src/**/*.js\"",
    "start": "concurrently \"yarn run dev:server\" \"yarn run dev:bundler\"",
    "dev:server": "cross-env NODE_PATH=./src nodemon --exec \"babel-node src/server/server.js\" --ignore .reactful.json --ignore public/",
    "dev:bundler": "webpack -wd",
    "build:react": "cross-env NODE_ENV=production webpack --progress -p",
    "build:node": "babel src -d build --config-file ./babel-node.config.js --copy-files",
    "build:all": "yarn install && yarn run build:react && yarn run build:node",
    "prod:start": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start -i max build/server/server.js --update-env --name telemedicineProd",
    "prod:stop": "pm2 stop telemedicineProd",
    "prod:reload": "pm2 reload --update-env telemedicineProd",
    "prod:logs": "pm2 logs --update-env telemedicineProd",
    "test": "jest",
    "verify-tests": "jest --coverage",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "knex": "knex",
    "knex:m:m": "npm run knex -- migrate:make",
    "knex:m:l": "npm run knex -- migrate:latest",
    "knex:m:r": "npm run knex -- migrate:rollback",
    "knex:s:m": "npm run knex -- seed:make",
    "knex:s:r": "npm run knex -- seed:run",
    "knex:refresh": "npm run knex:m:r && npm run knex:m:l",
    "knex:refresh:seed": "npm run knex:refresh && npm run knex:s:r"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "ejs": "^3.1.5",
    "express": "4",
    "morgan": "^1.10.0",
    "pm2": "^4.4.1",
    "react": "16",
    "react-dom": "16",
    "regenerator-runtime": "^0.13.7",
    "serialize-javascript": "^4.0.0",
    "@godaddy/terminus": "^4.4.1",
    "bcrypt": "^5.0.0",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-jwt": "^6.0.0",
    "express-jwt-permissions": "^1.3.3",
    "express-validator": "^6.6.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "nodemailer": "^6.4.11",
    "pg": "^8.3.0",
    "rate-limiter-flexible": "^2.1.10",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "reactful": "^3.1.3",
    "coveralls": "^3.1.0",
    "supertest": "^4.0.2",
    "style-loader": "^1.2.1",
    "css-loader": "^4.2.1",
    "webpack": "4",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.12",
    "mini-css-extract-plugin": "^0.10.0"
  }
}